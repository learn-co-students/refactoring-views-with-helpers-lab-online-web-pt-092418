<ul>
<% @songs.each do |song| %>
  <li>
    <%= link_to song.title, song_path(song) %>
    
    <%= display_artist(song) %>
    <% if song.artist %>
      <%= link_to song.artist.name, artist_path(song.artist) %>
    <% else %>
      <%= link_to "Add Artist", edit_song_path(song) %>
    <% end %>
  </li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
